  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --fix",
    "*.{css,scss,svelte}": "stylelint --fix",
    "*.{js,cjs,ts,css,scss,html,svelte,json,md,yml}": "prettier --write"
  },
  "packageManager": "yarn@3.0.1",
  "private": true,
  "repository": "github:Freemindtronic/evicypher-webmail",
  "scripts": {
    "build": "yarn build:eleventy-rollup && yarn build:static && yarn build:parcel && node manifest.js --patch",
    "build-storybook": "build-storybook -s extension",
    "build:eleventy-rollup": "eleventy --input=src/pages --output=build & rollup -c &",
    "build:parcel": "parcel build 'build/{*.html,*.gif,manifest.json}' --target extension",
    "build:static": "mkdir -p build extension && cp -r static/* extension & node manifest.js &",
    "check": "tsc --noEmit & eslint --fix 'src/**/*.{ts,js,svelte}' & stylelint --fix 'src/**/*' & svelte-check --tsconfig ./tsconfig.json &",
    "check:ci": "tsc --noEmit && eslint --fix 'src/**/*.{ts,js,svelte}' && stylelint --fix 'src/**/*' && svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf build extension",
    "doc": "typedoc src --sort source-order --listInvalidSymbolLinks",
    "postinstall": "husky install",
    "release": "standard-version",
    "start": "mkdir -p build extension && node manifest.js && cp -r static/* extension & eleventy --watch --input=src/pages --output=build & rollup -c -w & parcel watch 'build/{*.html,*.gif,manifest.json}' --no-hmr --target extension-dev &",
    "storybook": "start-storybook -p 6006 -s extension",
    "test": "cypress run -b firefox"
  },
  "targets": {
    "extension-dev": {
      "context": "browser",
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      },
      "distDir": "./extension"
    },
    "extension": {
      "context": "browser",
      "sourceMap": false,
      "distDir": "./extension"
    }
  },
  "alias": {
    "$": "./src",
    "openpgp": "openpgp/dist/openpgp.mjs"
  },
  "type": "module",
  "volta": {
    "node": "16.10.0",
    "yarn": "1.22.15"
  }
}
