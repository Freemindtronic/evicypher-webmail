{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf extension/build",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "check": "tsc --noEmit && eslint --fix 'src/**/*.{ts,js,svelte}' && stylelint --fix 'src/**/*' && svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "husky install",
    "test": "echo 'No tests defined' && exit 1",
    "doc": "typedoc src"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --fix",
    "*.{css,scss,svelte}": "stylelint --fix",
    "*.{js,ts,css,scss,html,svelte,json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "axlsign": "git+ssh://git@github.com:Freemindtronic/lib_axlsign",
    "base64-arraybuffer": "^0.2.0",
    "crypto-js": "^4.0.0",
    "debug": "^4.3.2",
    "postcss": "^8.3.5",
    "qrcode": "^1.4.4",
    "rollup": "^2.53.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.2",
    "svelte": "^3.38.3",
    "svelte-burger-menu": "git+ssh://git@github.com:jorisBarkema/svelte-burger-menu",
    "svelte-i18n": "^3.3.9",
    "svelte-preprocess": "^4.7.4",
    "svelte-spa-router": "^3.2.0",
    "tippy.js": "^6.3.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webextension-polyfill-ts": "^0.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/debug": "^4.1.6",
    "@types/node": "^16.3.1",
    "@types/qrcode": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@wemake-services/stylelint-config-scss": "^0.5.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.37.0",
    "eslint-config-xo-typescript": "^0.43.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "eslint-plugin-svelte3": "^3.2.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-jsdoc": "^0.3.23",
    "prettier-plugin-svelte": "^2.3.1",
